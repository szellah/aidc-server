{
    "source": {
      "include": ["./serverResponses"],
      "includePattern": "Location.js",
      "excludePattern": "(node_modules/|docs)"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "recurse": "true",
        "destination": "./docs/"
    }
}